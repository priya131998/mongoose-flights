
<%- include('../partials/header') %>

<form action ="/flights/<%= flightId %>/tickets" method="POST">
	<label>Seat:
	<input name="seat" required pattern="[A-F][1-9]\d?">
    </label><br>
	<label>Price:
	<input type="number" name="price" />
    </label><br>
	<input type="submit" value="Add Ticket" />
</form>